
  <%for yto in @items: %>

<div class="row">
  <div class="span3">
      <a id="<%=yto.media$group.yt$videoid.$t %>" class="srlink pull-left">
        <% thumbnail = yto.media$group.media$thumbnail[0]%>
        <div style="width: 120px; height: 70; overflow: hidden;">
          <img  style="position: relative; top -10px;" src="<%=thumbnail.url%>" >
        </div>
      </a>
      <div>
        <p><%= yto.title.$t %></p>
      </div>
      </div>
</div>
  <% end %>


